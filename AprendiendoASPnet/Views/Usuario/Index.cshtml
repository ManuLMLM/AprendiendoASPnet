
@model List<Usuario>

    <h1>Usuarios</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m=>item.Email)</td>
            </tr>
        }
        </tbody>
    </table>